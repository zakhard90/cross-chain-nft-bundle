{
  "printWidth": 120,
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "bracketSpacing": true
      }
    },
    {
      "files": "*.sol",
      "options": {
        "bracketSpacing": true,
        "singleQuote": false
      }
    }
  ]
}
